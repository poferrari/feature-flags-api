{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "TokenConfigurations": {
    "Audience": "ExemploAudience",
    "Issuer": "ExemploIssuer",
    "Seconds": 60
  },
  "FeatureManagement": {
    "WeatherForecast": true,
    "WeatherForecastPost": true,
    "WeatherForecastGet": false,
    "WeatherForecastGetById": true,
    "Promotion": {
      "EnabledFor": [
        {
          "Name": "Microsoft.TimeWindow",
          "Parameters": {
            "Start": "09 Apr 2021 02:00:00 +00:00",
            "End": "11 Apr 2021 01:00:00 +00:00"
          }
        }
      ]
    },
    "WeatherForecastGetByIdTest": {
      "EnabledFor": [
        {
          "Name": "Microsoft.Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    }
  }
}
